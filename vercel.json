{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "dist/server/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "dist/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
} 